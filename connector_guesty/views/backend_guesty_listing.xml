<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="backend_guesty_listing_tree_view" model="ir.ui.view">
        <field name="name">backend.listing.tree</field>
        <field name="model">backend.guesty.listing</field>
        <field name="arch" type="xml">
            <tree>
                <field name="nickname" />
                <field name="name" />
                <field name="property_ids" widget="many2many_tags" />
                <field name="external_id" />
            </tree>
        </field>
    </record>
</odoo>
